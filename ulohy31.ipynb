    "class Ball:\n",
    "    def __init__(self, bounce_factor=0.6):\n",
    "       \n",
    "        self.bounce_factor = bounce_factor\n",
    "    \n",
    "    def hop_height(self, h, k):\n",
    "        \n",
    "        if k < 0:\n",
    "            raise ValueError(\"캛칤slo skoku mus칤 by콘 nez치porn칠\")\n",
    "        \n",
    "        # V칳코ka kles치 exponenci치lne: h * (bounce_factor)^k\n",
    "        return h * (self.bounce_factor ** k)\n",
    "\n",
    "\n",
    "# Pr칤klad pou쬴tia\n",
    "if __name__ == \"__main__\":\n",
    "    # Vytvor칤me loptu s koeficientom odrazu 0.6 (60%)\n",
    "    ball = Ball(bounce_factor=0.6)\n",
    "    \n",
    "    # Po캜iato캜n치 v칳코ka\n",
    "    initial_height = 10  # metre\n",
    "    \n",
    "    print(f\"Po캜iato캜n치 v칳코ka: {initial_height} m\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    # Vypo캜칤tame prv칳ch 5 skokov\n",
    "    for k in range(6):\n",
    "        height = ball.hop_height(initial_height, k)\n",
    "        print(f\"Skok {k}: {height:.3f} m\")\n",
    "    \n",
    "    print(\"\\n\" + \"=\" * 40)\n",
    "    \n",
    "    # Pr칤klad s inou loptou (vy코코ia odrazivos콘)\n",
    "    bouncy_ball = Ball(bounce_factor=0.8)\n",
    "    print(f\"\\nOdrazivej코ia lopta (koeficient 0.8):\")\n",
    "    print(f\"V칳코ka 3. skoku z 10m: {bouncy_ball.hop_height(10, 3):.3f} m\")"
